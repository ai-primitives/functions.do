{
  "name": "functions-do",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "test": "vitest",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.1.15",
    "@ai-sdk/openai-compatible": "^0.1.13",
    "@openrouter/ai-sdk-provider": "^0.4.3",
    "@payloadcms/db-mongodb": "^3.28.1",
    "@payloadcms/email-nodemailer": "^3.28.1",
    "@payloadcms/graphql": "^3.28.1",
    "@payloadcms/next": "^3.28.1",
    "@payloadcms/payload-cloud": "^3.28.1",
    "@payloadcms/plugin-multi-tenant": "^3.28.1",
    "@payloadcms/richtext-lexical": "^3.28.1",
    "@types/js-yaml": "^4.0.9",
    "@vercel/functions": "^2.0.0",
    "ai": "^4.1.47",
    "ai-functions": "^0.3.0",
    "ai-workflows": "^1.1.0",
    "camelcase-keys": "^9.1.3",
    "cross-env": "^7.0.3",
    "graphql": "^16.8.1",
    "js-yaml": "^4.1.0",
    "lodash-es": "^4.17.21",
    "next": "15.1.5",
    "payload": "^3.28.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "sharp": "0.32.6",
    "yaml": "^2.7.0",
    "zod": "^3.24.2",
    "zod-to-json-schema": "^3.24.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.5.4",
    "@types/react": "19.0.7",
    "@types/react-dom": "19.0.3",
    "dotenv": "^16.4.7",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.1.5",
    "prettier": "^3.4.2",
    "typescript": "5.7.3",
    "vitest": "^3.0.8"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 180,
    "semi": false,
    "jsxSingleQuote": true,
    "jsxBracketSameLine": true
  }
}
